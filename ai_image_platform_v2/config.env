# AI图像处理平台环境配置

# 后端服务配置
FLASK_PORT=5002
FLASK_ENV=development
SECRET_KEY=ai-image-platform-secret-key-2024

# 数据库配置
DATABASE_URL=sqlite:///ai_image_platform.db

# JWT配置
JWT_SECRET_KEY=jwt-secret-key-2024

# 文件上传配置
MAX_CONTENT_LENGTH=16777216
UPLOAD_FOLDER=backend/static/uploads

# AI模型配置
QWEN_API_KEY=
QWEN_MODEL_NAME=qwen-turbo
QWEN_VL_MODEL_NAME=qwen-vl-plus

# Redis配置（可选）
REDIS_URL=redis://localhost:6379/0

# 日志配置
LOG_LEVEL=INFO
LOG_FILE=logs/app.log
